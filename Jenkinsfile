pipeline {
    agent any 
    stages {
       stage("Build") {
	  steps {
	    echo "hello, this is the build stage.hello, this is the build stage"
	  }
       }

       stage("Test") {
	  steps {
        echo "hello, this is the build stage.hello, this is the build stage.hello, this is the build stage"
	  }
       }

       stage("deploy") {
	  steps {
		echo " hello, hello, this is the build stage."
	  }
       }
   }
}
